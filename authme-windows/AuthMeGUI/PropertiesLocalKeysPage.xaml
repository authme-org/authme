<Page x:Class="AuthMeGUI.PropertiesLocalKeysPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AuthMeGUI"
      mc:Ignorable="d" 
      d:DesignHeight="240" d:DesignWidth="359"
      Title="PropertiesLocalKeysPage">

    <Grid>
        <Border BorderBrush="#FFC3C3C3" BorderThickness="1" HorizontalAlignment="Left" Height="55" Margin="14,32,0,0" VerticalAlignment="Top" Width="326"/>
        <TextBox x:Name="keyFileTextBox" HorizontalAlignment="Left" Height="23" Margin="22,55,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="311"/>
        <Label x:Name="keyFileLabel" Content="Key Filename" HorizontalAlignment="Left" Margin="142,17,0,0" VerticalAlignment="Top" Background="White"/>
        <TextBlock x:Name="keyFileTextBlock" HorizontalAlignment="Left" Height="30" Margin="36,104,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Width="286"/>
        <Button x:Name="generateKeyButton" Content="Generate" HorizontalAlignment="Left" Margin="147,169,0,0" VerticalAlignment="Top" Width="75" Click="generateKeyButton_Click"/>
    </Grid>
</Page>
